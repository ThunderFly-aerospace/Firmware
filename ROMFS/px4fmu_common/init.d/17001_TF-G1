#!nsh
#
# @name ThunderFly G1
#
# @type AutoGyro
# @class Autogyro
#
# @output MAIN1 ruddle_left
# @output MAIN2 ruddle_right
# @output MAIN3 rotor_left 
# @output MAIN4 rotor_right
# @output MAIN5 throttle
# @output MAIN6 rotor_motor
#
# @maintainer ThunderFly
#

sh /etc/init.d/rc.rw_defaults

if [ $AUTOCNF == yes ]
then
	param set BAT_CAPACITY 5500.0
	param set BAT_N_CELLS 4
	param set BAT_V_CHARGED 4.199999809265137


set BAT_CAPACITY 5500.0
set BAT_N_CELLS 4
set BAT_V_CHARGED 4.199999809265137

set FW_AIRSPD_MIN 5.0

set FW_MAN_P_MAX 20.0
set FW_T_SPDWEIGHT 0.5

set MIXER TF-G1


set COM_ARM_WO_GPS 0
set PWM_MAIN_DIS8 1000


